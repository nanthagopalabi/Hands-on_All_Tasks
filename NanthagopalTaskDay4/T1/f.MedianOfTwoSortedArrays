 (function() {     //Using IIFE function
  let arr1=[1,2,12,13,15];
  let arr2=[17,26,30,38,45];
  console.log("Using IIFE function :");
  let arr3 = arr1.concat(arr2).sort((a,b)=>a-b);
  let sum = arr3.length
  let middle1=(arr3[Math.floor(sum/2)]);
  let middle2=(arr3[(Math.floor(sum/2)-1)]);
  console.log((middle1+middle2)/2);
  })();
  
//**********//

var name=function() {   //Using anonymous function 
  let arr1=[1,2,12,13,15];
  let arr2=[17,26,30,38,45];
  console.log("Using anonymous function :");
  let arr3 = arr1.concat(arr2).sort((a,b)=>a-b);
  let sum = arr3.length
  let middle1=(arr3[Math.floor(sum/2)]);
  let middle2=(arr3[(Math.floor(sum/2)-1)]);
  console.log((middle1+middle2)/2);
  }
  name();