 (function() {     //Using IIFE function
  let arr=[1,2,3,4,5,7,8,9,10];
  console.log("Using IIFE function :");
  for (let i =0; i<=arr.length; i++) {
    let flag = 0;
  for (let j = 2; j <i; j++) {
        if (i % j == 0) {
            flag = 1;
            break;
        }
    }
    if (i > 1 && flag == 0) {
        console.log([i]);
    }
}return;
})();

//**********//

function name() {   //Using anonymous function 
  let arr1=[1,2,3,4,5,7,8,9,10];
  console.log("Using anonymous function :");
  for (let i =0; i<=arr1.length; i++) {
    let flag = 0;
  for (let j = 2; j <i; j++) {
        if (i % j == 0) {
            flag = 1;
            break;
        }
    }
    if (i > 1 && flag == 0) {

        console.log([i]);
    }
  }
}name();